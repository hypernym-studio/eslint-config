{
  "name": "@hypernym/eslint-config",
  "version": "3.6.3",
  "author": "Hypernym Studio",
  "description": "Hypernym's internal config for ESLint.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hypernym-studio/eslint-config.git"
  },
  "homepage": "https://github.com/hypernym-studio/eslint-config",
  "funding": "https://github.com/sponsors/ivodolenc",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./svelte": {
      "types": "./dist/svelte/index.d.mts",
      "import": "./dist/svelte/index.mjs"
    },
    "./vue": {
      "types": "./dist/vue/index.d.mts",
      "import": "./dist/vue/index.mjs"
    },
    "./react": {
      "types": "./dist/react/index.d.mts",
      "import": "./dist/react/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "eslint",
    "eslint-config",
    "presets",
    "config",
    "esm"
  ],
  "scripts": {
    "build": "hyperbundler",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "eslint-plugin-react": ">=7.33.0",
    "eslint-plugin-react-hooks": ">=4.4.0",
    "eslint-plugin-react-refresh": ">=0.4.0",
    "eslint-plugin-svelte": ">=3.0.0",
    "eslint-plugin-vue": ">=10.0.0",
    "vue-eslint-parser": ">=10.0.0",
    "svelte-eslint-parser": ">=1.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "globals": "^16.3.0"
  },
  "devDependencies": {
    "@hypernym/bundler": "^0.14.4",
    "@hypernym/prettier-config": "^3.2.5",
    "@hypernym/tsconfig": "^2.6.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-svelte": "^3.11.0",
    "eslint-plugin-vue": "^10.4.0",
    "prettier": "^3.6.2",
    "svelte-eslint-parser": "^1.3.1",
    "typescript": "^5.9.2",
    "vue-eslint-parser": "^10.2.0"
  }
}
